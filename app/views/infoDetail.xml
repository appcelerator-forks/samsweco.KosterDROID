<Alloy>
	<Collection src="info_urlModel"></Collection>
	<Window id="infoDetail" class="container">
		<ScrollView class="headView" scrollType="vertical" showVerticalScrollIndicator="true">
			<View id="slideView">
				<ImageView id="infoImg"></ImageView>
			</View>
			<Label id="lblInfoTitle"></Label>
			<Label id="lblInfoText"></Label>
			<Label id="lblLink" text="Klicka på länken nedan för att se mer information: "/>
			<View id="linkView">
			<TableView id="tableView" onClick="getLink">
				<TableViewRow class="row" onClick="getLink" />
			</TableView>
			</View>
		</ScrollView>
	</Window>
</Alloy>